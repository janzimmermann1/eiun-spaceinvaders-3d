                        <ui:Button text="Start Hard Level" parse-escape-sequences="true" display-tooltip-when-elided="true" name="start-hard-button" style="margin-bottom: 2px; font-size: 24px; -unity-text-align: middle-center; white-space: nowrap; text-overflow: clip; border-top-left-radius: 2px; border-top-right-radius: 2px; border-bottom-right-radius: 2px; border-bottom-left-radius: 2px; border-top-width: 0; border-right-width: 0; border-bottom-width: 0; border-left-width: 0; background-color: rgba(0, 0, 0, 0.78); color: rgb(255, 255, 255);" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:Button text="Generate new terrain" parse-escape-sequences="true" display-tooltip-when-elided="true" name="new-terrain-button" style="margin-bottom: 20px; font-size: 24px; background-color: rgba(0, 0, 0, 0.78); border-top-width: 0; border-right-width: 0; border-bottom-width: 0; border-left-width: 0; border-top-left-radius: 2px; border-top-right-radius: 2px; border-bottom-right-radius: 2px; border-bottom-left-radius: 2px; color: rgb(255, 255, 255);" />
