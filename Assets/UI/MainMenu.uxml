<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="background" style="flex-grow: 1; background-color: rgba(255, 255, 255, 0.12);">
        <ui:VisualElement style="flex-grow: 1;" />
        <ui:Label tabindex="-1" text="Space Invaders - 3D" parse-escape-sequences="true" display-tooltip-when-elided="true" name="title" style="align-items: center; align-self: center; -unity-font: initial; -unity-font-style: bold; font-size: 32px; margin-top: 20px; color: rgb(118, 119, 118);" />
        <ui:VisualElement style="flex-grow: 1;" />
        <ui:VisualElement name="center" style="flex-grow: 1; align-items: center; align-self: auto; flex-direction: row; margin-right: 5%; margin-bottom: 0; margin-left: 5%; justify-content: flex-start;">
            <ui:VisualElement name="content" style="flex-grow: 1; align-self: auto; align-items: stretch; flex-direction: column; justify-content: flex-start;">
                <ui:VisualElement name="level-selector" style="flex-grow: 1; justify-content: flex-start; align-items: flex-start; flex-direction: row; flex-wrap: nowrap; align-self: auto; display: flex; -unity-text-align: upper-left; position: relative; opacity: 1; overflow: visible; visibility: visible; text-overflow: clip; white-space: normal; height: auto; max-height: 80px;">
                    <ui:Label tabindex="-1" text="Choose level" parse-escape-sequences="true" display-tooltip-when-elided="true" name="choose-level-label" style="-unity-font-style: bold; font-size: 24px; color: rgb(128, 128, 128); -unity-text-align: middle-center; height: 65px;" />
                    <ui:VisualElement name="button-group" style="flex-grow: 1; flex-direction: row; justify-content: space-around; align-items: center; align-self: center;">
